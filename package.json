{
  "name": "latex-validation",
  "private": false,
  "version": "0.0.1",
  "license": "ISC",
  "main": "dist/index.js",
  "bin": {
    "pdf-ref-resolver": "dist/index.js"
  },
  "scripts": {
    "clean": "rm -r dist/",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "alias": "./node_modules/.bin/babel --config-file ./.babelrc-path-alias ./dist -d ./dist",
    "lint": "eslint src/",
    "tsup": "tsup --onSuccess 'run-p alias lint'",
    "tsup:watch": "tsup --watch src/ --onSuccess 'run-p alias lint'",
    "watch": "run-p tsc:watch tsup:watch",
    "build": "run-p tsc tsup",
    "bundle": "NODE_ENV=prod tsup --clean",
    "rebuild": "run-s clean build"
  },
  "dependencies": {
    "@koa/router": "~12.0.0",
    "fs-extra": "^11.1.1",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-json": "^2.0.2",
    "lodash": "^4.17.21",
    "nconf": "^0.12.0",
    "tslib": "^2.5.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "~7.22.9",
    "@babel/preset-env": "~7.22.9",
    "@babel/preset-typescript": "~7.22.5",
    "@types/crypto-js": "^4.1.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.1",
    "@types/koa": "^2.13.6",
    "@types/koa-json": "^2.0.20",
    "@types/koa__router": "~12.0.0",
    "@types/lodash": "^4.14.194",
    "@types/nconf": "^0.10.3",
    "@types/node": "18.16.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "axios": "~1.4.0",
    "babel-jest": "~29.6.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5"
  }
}
